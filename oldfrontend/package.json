{
  "name": "jupbox",
  "version": "1.0.0",
  "description": "Electron app for Jupbox with Next.js frontend",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start:next": "set NODE_ENV=development && electron .",
    "dev": "set NODE_ENV=development && electron .",
    "dev:next": "set NODE_ENV=development && electron .",
    "build": "cd frontend && npm run build:static && electron-builder",
    "build:next": "cd frontend && npm run build:static && electron-builder",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build:static",
    "frontend:start": "cd frontend && npm start"
  },
  "keywords": [
    "electron",
    "nextjs",
    "spotify",
    "music",
    "gesture-control"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "next": "^15.4.6",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  }
}
